{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "scripts": {
    "build": "bun run generate-routes && bun with-env bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "bun with-env drizzle-kit migrate --config drizzle.config.ts",
    "db:push": "bun with-env drizzle-kit push --config drizzle.config.ts",
    "db:seed": "bun run src/db/seed.ts",
    "db:studio": "bun with-env drizzle-kit studio --config drizzle.config.ts",
    "dev": "bun run --bun concurrently \"bun run watch-routes\" \"bun --hot src/index.tsx\"",
    "format": "prettier -uwl --cache .",
    "format:check": "prettier -uc --cache .",
    "generate-routes": "tsr generate",
    "lint": "eslint . --max-warnings 0 --cache --cache-location ./node_modules/.cache/eslint",
    "lint:fix": "eslint . --fix --cache --cache-location ./node_modules/.cache/eslint",
    "prepare": "husky",
    "start": "NODE_ENV=production bun src/index.tsx",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit --extendedDiagnostics",
    "watch-routes": "tsr watch",
    "with-env": "dotenvx run --"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@tanstack/react-query": "^5.80.5",
    "@tanstack/react-router": "^1.120.15",
    "@trpc/client": "^11.3.0",
    "@trpc/react-query": "^11.3.0",
    "@trpc/server": "^11.3.0",
    "@trpc/tanstack-react-query": "^11.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.2",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.511.0",
    "nanoid": "^5.1.5",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.7",
    "radix-ui": "^1.4.2",
    "react": "^19.1.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "react-resizable-panels": "^3.0.2",
    "recharts": "^2.15.3",
    "sonner": "^2.0.5",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8",
    "vaul": "^1.1.2",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.44.1",
    "@eslint-react/eslint-plugin": "^1.51.0",
    "@happy-dom/global-registrator": "^17.6.3",
    "@tanstack/eslint-plugin-query": "^5.78.0",
    "@tanstack/eslint-plugin-router": "^1.115.0",
    "@tanstack/react-query-devtools": "^5.80.5",
    "@tanstack/react-router-devtools": "^1.120.15",
    "@tanstack/router-cli": "^1.120.15",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "bun-plugin-dotenvx": "^0.6.3",
    "bun-plugin-tailwind": "^0.0.15",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import-x": "^4.15.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tw-animate-css": "^1.3.4",
    "typescript-eslint": "^8.33.1"
  },
  "packageManager": "bun@1.2.15",
  "trustedDependencies": [
    "better-sqlite3",
    "esbuild",
    "unrs-resolver"
  ]
}
